<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.tjsid.fieldwork">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".views.MainActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".views.InsertActivity"
                android:label="@string/title_activity_insert"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".views.CadUserActivity"
                android:label="@string/title_activity_cad_user"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".views.ConsultActivity"
                android:label="@string/title_activity_consult"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".views.ConsultaNotasActivity"
                android:label="@string/title_activity_consulta_notas"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".views.EstudosActivity"
                android:label="@string/title_activity_estudos"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".views.ResumoAnualActivity"
                android:label="@string/title_activity_resumo_anual"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".views.InsertNoteAcitivity">
        </activity>
    </application>

</manifest>